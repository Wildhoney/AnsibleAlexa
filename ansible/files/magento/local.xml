            <request_path><![CDATA[]]></request_path> <!-- this will almost always be blank -->
            <!--
                Examples - If your base_url looks like:
                    http://www.tinybrick.com                then    ''
                    http://www.tinybrick.com/tempstore      then    '/tempstore'
            -->
           <params>gclid,pcrid,mkwid,plid,kword,match,utm_medium,utm_source,utm_campaign,utm_term,utm_content,aa_set_site</params> <!-- comma separated list of url params to ignore such as get parameters for ads -->
           <params_nocache>SID,gclid,pcrid,mkwid,plid,kword,match,utm_medium,utm_source,utm_campaign,utm_term,utm_content</params_nocache> <!-- comma separated list of url params that won't be able to create a cached page (e.g. to avoid  -->
           <admin_uri><![CDATA[alexpie]]></admin_uri> <!-- the URI used to access the admin; empty defaults to 'admin' -->
           <exclude_uri>store,from_store,SID,sgps,sgpsSecure,paypal,aa_currency</exclude_uri> <!-- comma separated list of URI keywords which redirect immediately to index.php -->

           <multi_storeview>1</multi_storeview> <!-- Set to 1 if you have multiple views for a website under the same hostname, defaults to false "0" -->
           <multi_hostname>1</multi_hostname> <!-- empty defaults to single site -->
           <multi_hostname_config> <!-- Specify how to call Mage for each site. Use only if '<multi_hostname>' is set to 1' -->
                   <site> <!-- define multiple sites if you are using different codes -->
                       <run_code><![CDATA[website_uk]]></run_code> <!-- 'code' to run Mage as found in the Website or Shop View -->
                       <run_type><![CDATA[website]]></run_type> <!-- website|store -->
                       <host_name>{{ host_magento }}</host_name> <!-- comma separated list of host names, which will run Mage with the specified 'run_code' and 'run_type' -->
                   </site>
                   <site> <!-- can define multiple sites -->
                       <run_code><![CDATA[website_usa]]></run_code> <!-- 'code' to run Mage as found in the Website or Shop View -->
                       <run_type><![CDATA[website]]></run_type> <!-- website|store -->
